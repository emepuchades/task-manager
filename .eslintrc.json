{
    "env": {
        "browser": true,
        "es2021": true
    },
    "settings": {
        "detect": {
            "version": "detect"
        }
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:react/jsx-runtime",
        "standard",
        "eslint-config-prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "react/prop-types": 0
    }
}
